{
  "name": "@liquid-labs/semver-plus",
  "version": "1.0.0-alpha.11",
  "description": "Adds additional functionality to and fixes limitations in the base semver package.",
  "main": "dist/semver-plus.js",
  "scripts": {
    "build": "make build",
    "doc": "make doc",
    "lint": "make lint",
    "lint:fix": "make lint-fix",
    "test": "make test",
    "preversion": "npm run qa && npm run doc",
    "prepack": "npm run build",
    "qa": "make qa"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "semver",
    "version",
    "versioning"
  ],
  "author": "Zane Rockenbaugh <zane@liquid-labs.com>",
  "license": "Apache-2.0",
  "repository": {
    "url": "git+ssh://git@github.com/liquid-labs/semver-plus.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/liquid-labs/semver-plus/issues"
  },
  "homepage": "https://github.com/liquid-labs/semver-plus#readme",
  "devDependencies": {
    "@liquid-labs/dmd": "^7.1.1",
    "@liquid-labs/jsdoc-to-markdown": "^9.1.3",
    "@liquid-labs/sdlc-resource-babel-and-rollup": "^1.0.0-alpha.5",
    "@liquid-labs/sdlc-resource-eslint": "^1.0.0-alpha.2",
    "@liquid-labs/sdlc-resource-jest": "^1.0.0-alpha.5",
    "dmd-readme-api": "^1.0.0-beta.8"
  },
  "liq": {
    "packageType": "node|lib"
  },
  "dependencies": {
    "http-errors": "^2.0.0",
    "semver": "^7.0.0"
  },
  "_npm-check-plus": {
    "depcheck": {
      "ignoreMatches": [
        "@liquid-labs/sdlc-resource-*"
      ]
    }
  }
}
